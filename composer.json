{
    "name": "krissman/nova-mail-tracker",
    "description": "Laravel Nova tool to list & view sent emails",
    "keywords": [
        "laravel",
        "nova",
        "resource"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0.0",
        "jdavidbakr/mail-tracker": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Krissman\\NovaMailTracker\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Krissman\\NovaMailTracker\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
